extends layout

block styles
	link(type='text/css' rel='stylesheet' href='/stylesheets/wedding.css')
	link(type='text/css' rel='stylesheet' href='/stylesheets/polaroids.css')
	link(type='text/css' rel='stylesheet' href='/stylesheets/timeline.css')
