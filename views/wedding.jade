block content
	div.row
		div.col-sm-10.col-sm-offset-1
			div.center
				h1 <span>B</span>ride and <span>G</span>room
				ul.polaroids
					li <img src="/images/entourage/Joanne.jpg" /><span>Joanne</span>
					li <img src="/images/entourage/Herald.jpg" /><span>Herald</span>

			div.row
				div.col-md-12
					ul.timeline
						li 	
							div.timeline-badge <i class='fa fa-building'></i>
							div.timeline-panel
								div.timeline-heading 
									h4.timeline-title <i class='fa fa-calendar'></i> 2010
								div.timeline-body
									p Worked at Shoppers Drug Mart together
						li
							div.timeline-badge <i class='fa fa-birthday-cake'></i>
							div.timeline-panel
								div.timeline-heading
									h4.timeline-title <i class='fa fa-calendar'></i> February 2012
								div.timeline-body
									p First time celebrating Joanne's birthday together
									p Joanne graduated from University of Manitoba
						li
							div.timeline-badge <i class='fa fa-heart'></i>
							div.timeline-panel
								div.timeline-heading
									h4.timeline-title <i class='fa fa-calendar'></i> May 2012
								div.timeline-body
									p Officially a couple
