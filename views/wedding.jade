							div.timeline-badge <i class='fa fa-plane'></i>
							div.timeline-panel
								div.timeline-heading
									h4.timeline-title <i class='fa fa-calendar'></i> November 2016
								div.timeline-body
									p Travelled to the Philippines
						li
							div.timeline-badge <i class='fa fa-diamond'></i>
							div.timeline-panel
								div.timeline-heading
									h4.timeline-title <i class='fa fa-calendar'></i> February 2017
								div.timeline-body
									p Engaged
						li
							div.timeline-badge <i class='fa fa-diamond'></i>
							div.timeline-panel
								div.timeline-heading
									h4.timeline-title <i class='fa fa-calendar'></i> June 30, 2018
								div.timeline-body
									p I DO

